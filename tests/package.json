{
  "name": "tsoa-tests",
  "private": true,
  "description": "Build swagger-compliant REST APIs using TypeScript and Node",
  "version": "4.1.3",
  "keywords": [
    "typescript",
    "openapi",
    "swagger",
    "server",
    "node",
    "node.js",
    "codegen",
    "generation",
    "express",
    "hapi.js",
    "koa"
  ],
  "scripts": {
    "clean": "rimraf dist fixtures/*/routes.ts fixtures/*/customRoutes.ts",
    "prepare-test": "cross-env NODE_ENV=tsoa_test ts-node --require=tsconfig-paths/register ./prepare.ts",
    "pretest": "yarn clean && yarn prepare-test && yarn typecheck",
    "test": "cross-env NODE_ENV=tsoa_test mocha --require=ts-node/register --require=tsconfig-paths/register --exit",
    "typecheck": "tsc"
  },
  "author": "Luke Autry <lukeautry@gmail.com> (http://www.lukeautry.com)",
  "license": "MIT",
  "devDependencies": {
    "@hapi/hapi": "^20.2.1",
    "@koa/multer": "^3.0.0",
    "@koa/router": "^8.0.8",
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.2.0",
    "@types/express": "^4.17.1",
    "@types/hapi__hapi": "^20.0.9",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__router": "^8.0.2",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^9.0.0",
    "@types/multer": "1.4.5",
    "@types/node": "^12.20.16",
    "@types/sinon": "^9.0.10",
    "@types/supertest": "^2.0.8",
    "body-parser": "^1.19.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "cross-env": "^5.1.6",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "method-override": "^3.0.0",
    "mocha": "^7.1.1",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "sinon": "^9.2.3",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeautry/tsoa.git"
  },
  "engines": {
    "yarn": ">=1.9.4",
    "node": ">=12.0.0"
  },
  "engineStrict": true
}
