{
  "name": "tsoa-tests",
  "private": true,
  "description": "Build swagger-compliant REST APIs using TypeScript and Node",
  "version": "5.0.0-alpha.3",
  "keywords": [
    "typescript",
    "openapi",
    "swagger",
    "server",
    "node",
    "node.js",
    "codegen",
    "generation",
    "express",
    "hapi.js",
    "koa"
  ],
  "scripts": {
    "clean": "rimraf dist fixtures/*/routes.ts fixtures/*/customRoutes.ts fixtures/*/custom-route-generator/routes/*",
    "prepare-test": "cross-env NODE_ENV=tsoa_test ts-node --require=tsconfig-paths/register ./prepare.ts",
    "pretest": "yarn clean && yarn prepare-test && yarn typecheck",
    "test": "cross-env NODE_ENV=tsoa_test mocha --require=ts-node/register --require=tsconfig-paths/register --exit",
    "typecheck": "tsc"
  },
  "author": "Luke Autry <lukeautry@gmail.com> (http://www.lukeautry.com)",
  "license": "MIT",
  "devDependencies": {
    "@hapi/hapi": "^20.2.2",
    "@koa/multer": "^3.0.0",
    "@koa/router": "^12.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/hapi__hapi": "^20.0.12",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa__router": "^8.0.11",
    "@types/method-override": "0.0.32",
    "@types/mocha": "^9.1.1",
    "@types/multer": "1.4.7",
    "@types/node": "^18.7.13",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "aws-cdk-lib": "^2.51.1",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "chalk": "^4.1.2",
    "constructs": "^10.1.169",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "method-override": "^3.0.0",
    "mocha": "^10.0.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeautry/tsoa.git"
  },
  "engines": {
    "yarn": ">=1.9.4",
    "node": ">=12.0.0"
  },
  "engineStrict": true
}
