{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "semicolon": false,
    "trailing-comma": false,
    "quotemark": false,
    "max-line-length": false,
    "no-unused-expression": false,
    "no-shadowed-variable": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-ordering": [
      false
    ],
    "max-classes-per-file": false,
    "no-reference": false,
    "no-namespace": [
      false
    ],
    "ban-types": false
  }
}